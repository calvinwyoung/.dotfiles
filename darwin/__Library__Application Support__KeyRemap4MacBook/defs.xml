<?xml version="1.0"?>
<root>
  <appdef>
    <appname>KEEPASSX</appname>
    <equal>org.keepassx.keepassx</equal>
  </appdef>

  <appdef>
    <appname>FINDER</appname>
    <equal>com.apple.finder</equal>
  </appdef>

  <appdef>
    <appname>PATH_FINDER</appname>
    <equal>com.cocoatech.PathFinder</equal>
  </appdef>

  <replacementdef>
    <replacementname>MODIFIER_FLAG_HYPER</replacementname>
    <replacementvalue>
      ModifierFlag::COMMAND_L | ModifierFlag::OPTION_L | ModifierFlag::CONTROL_L
    </replacementvalue>
  </replacementdef>

  <replacementdef>
    <replacementname>BROWSERS</replacementname>
    <replacementvalue>
      GOOGLE_CHROME,
      SAFARI,
      FIREFOX,
      OPERA
    </replacementvalue>
  </replacementdef>

  <replacementdef>
    <replacementname>EMACS_MODE_IGNORE_APPS</replacementname>
    <replacementvalue>
      ECLIPSE,
      EMACS,
      REMOTEDESKTOPCONNECTION,
      TERMINAL,
      VI,
      VIRTUALMACHINE,
      X11
    </replacementvalue>
  </replacementdef>

  <vkopenurldef>
    <name>
      KeyCode::VK_OPEN_URL_SHELL_warp_mouse_cursor_position_front_window_top_right
    </name>
    <url type="shell">
      <![CDATA[
               /Applications/Karabiner.app/Contents/Library/utilities/bin/warp-mouse-cursor-position front_window top -60 right 10
      ]]>
    </url>
  </vkopenurldef>
</root>
